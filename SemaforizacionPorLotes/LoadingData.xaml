<Window x:Class="SemaforizacionPorLotes.LoadingData"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SemaforizacionPorLotes"
        mc:Ignorable="d"
        Icon="/SEMAFORO4.png"
        ContentRendered="Window_ContentRendered"
        Title="Semaforización por lotes" d:DesignHeight="1080" d:DesignWidth="1920"
        WindowState="Maximized">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Image Source="reloj-de-arena.png" Height="300" HorizontalAlignment="Center" VerticalAlignment="Center"></Image>
            <TextBlock x:Name="StatusTB" FontSize="60" FontWeight="Bold" Margin="0 40 0 0" HorizontalAlignment="Center" VerticalAlignment="Center">Recolectando datos de QuickBooks...</TextBlock>
            <TextBlock FontSize="32" Margin="0 20 0 0" HorizontalAlignment="Center" VerticalAlignment="Center">Acepta cualquier cuadro de diálogo que aparezca en la interfaz de QuickBooks</TextBlock>
        </StackPanel>
    </Grid>
</Window>
